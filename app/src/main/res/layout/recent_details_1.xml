        <com.google.android.material.textfield.TextInputLayout

            android:id="@+id/textInputLayout3"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:layout_width="320dp"
            android:layout_height="42dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="370dp"
            android:hint="Additional Cell Number "
            app:boxCollapsedPaddingTop="10dp"
            app:boxStrokeColor="#B71C1C"
            app:circularflow_defaultRadius="5dp"
            app:hintTextColor="#383C3C"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout22">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/additional_cell"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/montserrat_medium"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textColorHighlight="#A55252"
                android:textColorHint="#5A5858"
                android:textColorLink="#B71C1C"
                android:textSize="12sp" />

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:id="@+id/client_last_appointment_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="Last Appointment Date:"
            android:textColor="@color/trucking_grey"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/client_last_appointment_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="120dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="Last Appointment Location:"
            android:textColor="@color/trucking_grey"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/last_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="260dp"
            android:layout_marginTop="120dp"
            android:fontFamily="@font/montserrat_semibold"
            android:textColor="@color/trucking_200"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/last_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="260dp"
            android:layout_marginTop="145dp"
            android:fontFamily="@font/montserrat_semibold"
            android:textColor="@color/trucking_200"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/flag_test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
