        <com.google.android.material.textfield.TextInputLayout

            android:id="@+id/textInputLayout224"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:layout_width="320dp"
            android:layout_height="42dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="310dp"
            android:hint="Cellphone Number"
            app:boxCollapsedPaddingTop="10dp"
            app:boxStrokeColor="#B71C1C"
            app:circularflow_defaultRadius="5dp"
            app:hintTextColor="#383C3C"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/client_name">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/client_number"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/montserrat_medium"

                android:inputType="number"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textColorHighlight="#A55252"
                android:textColorHint="#5A5858"
                android:textColorLink="#B71C1C"
                android:textSize="12sp" />

        </com.google.android.material.textfield.TextInputLayout>

        <EditText
            android:id="@+id/idEdtDate"
            android:layout_width="320dp"
            android:layout_height="42dp"
            android:layout_centerInParent="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="430dp"
            android:background="@drawable/datep_border"
            android:clickable="false"
            android:cursorVisible="false"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/montserrat_medium"
            android:hint="Date of Birth"
            android:padding="10dp"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/include3"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="@+id/include3"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout4" />

        <com.google.android.material.textfield.TextInputLayout


            android:id="@+id/textInputLayout6"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:layout_width="320dp"
            android:layout_height="42dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="490dp"
            android:hint="Client Company Name"
            app:boxCollapsedPaddingTop="10dp"
            app:boxStrokeColor="#B71C1C"
            app:circularflow_defaultRadius="5dp"
            app:hintTextColor="#383C3C"
            app:layout_constraintBottom_toBottomOf="@+id/include3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/radioGroup15"
            app:layout_constraintVertical_bias="0.054">

            <com.google.android.material.textfield.TextInputEditText

                android:id="@+id/company_name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/montserrat_medium"
                android:inputType="text"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textColorHighlight="#A55252"
                android:textColorHint="#5A5858"
                android:textColorLink="#B71C1C"
                android:textSize="12sp" />


        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="550dp"
            android:fontFamily="@font/montserrat_medium"
            android:text="Gender"
            android:textColor="#5A5858"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/idEdtDate"
            app:layout_constraintTop_toBottomOf="@+id/idEdtDate" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="320dp"
            android:layout_marginTop="60dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="New Client Details"
            android:textColor="@color/grey"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />


        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/montserrat_semibold"
            android:gravity="center"
            android:text="Client Details"
            android:textColor="#494949"
            android:textSize="40sp" />

        <Button
            android:id="@+id/client_nextbtn"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="680dp"
            android:layout_marginBottom="96dp"
            android:background="@drawable/ripple_effect_outline"
            android:fontFamily="@font/montserrat_semibold"
            android:shadowColor="#272424"
            android:text="Next"
            android:textColor="@drawable/btn_txt_color"
            android:textSize="12sp"
            app:cornerRadius="3dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/client_prevbtn"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="680dp"
            android:layout_marginBottom="96dp"
            android:background="@drawable/ripple_effect_outline"
            android:fontFamily="@font/montserrat_semibold"
            android:shadowColor="#272424"
            android:text="Previous"
            android:textColor="@drawable/btn_txt_color"
            android:textSize="12sp"
            app:cornerRadius="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/client_nextbtn"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />


        <RadioGroup
            android:id="@+id/radioGroup15"
            android:layout_width="310dp"
            android:layout_height="68dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="560dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7">

            <RadioButton
                android:id="@+id/Other"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/radio_selector"
                android:button="@android:color/transparent"
                android:elevation="4dp"
                android:fontFamily="@font/montserrat_medium"
                android:text="Other"
                android:textAlignment="center"
                android:textColor="@drawable/text_color"
                android:textSize="12sp"
                app:layout_constraintEnd_toStartOf="@+id/Other"
                app:layout_constraintHorizontal_bias="0.689"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <RadioButton
                android:id="@+id/male"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/radio_selector"
                android:button="@android:color/transparent"
                android:elevation="4dp"
                android:fontFamily="@font/montserrat_medium"
                android:text="Male"
                android:textAlignment="center"
                android:textColor="@drawable/text_color"
                android:textSize="12sp"
                app:layout_constraintEnd_toStartOf="@+id/female"
                app:layout_constraintHorizontal_bias="0.689"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <RadioButton
                android:id="@+id/female"
                android:layout_width="90dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="384dp"
                android:background="@drawable/radio_selector"
                android:button="@android:color/transparent"
                android:elevation="4dp"
                android:fontFamily="@font/montserrat_medium"
                android:text="Female"
                android:textAlignment="center"
                android:textColor="@drawable/text_color"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

        </RadioGroup>

